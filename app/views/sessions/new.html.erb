<h1>Sign in</h1>

<%= form_for @user, url: signin_path do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.collection_select :name, User.all, :name, :name, {prompt: "Select Name"} %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <p>
    <%= f.submit 'Sign In'%>
  </p>
<% end %>